{
  "devCommand": "npm start",
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "functions": {
    "api/isru-proxy.js": {
      "maxDuration": 10
    },
    "api/sneakerdb-proxy.js": {
      "maxDuration": 15
    },
    "api/universal-proxy.js": {
      "maxDuration": 15
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ]
}
